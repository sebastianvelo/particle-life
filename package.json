{
  "name": "particle-life",
  "version": "1.0.0",
  "description": "Particle Life",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "lucide-react": "^0.511.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "tw:build": "npx tailwindcss -i ./src/styles.css -o ./src/output.css",
    "tw:watch": "npx tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "start": "webpack serve --mode development",
    "build": "npm run tw:build && webpack --mode production",
    "dev": "webpack --mode development"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
